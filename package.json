{
  "name": "@mhmdtshref/nodejs-starter",
  "version": "1.0.0",
  "description": "NodeJS starter app",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build": "tsc",
    "lint:test": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "serve": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "dev": "cross-env NODE_ENV=dev APP_ENV=dev npm run serve",
    "local": "cross-env NODE_ENV=local APP_ENV=dev npm run serve",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhmdtshref/nodejs-starter.git"
  },
  "keywords": [
    "backend",
    "typescript",
    "nodejs",
    "starter",
    "boilerplate"
  ],
  "author": "Mohamed Sharif",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mhmdtshref/nodejs-starter/issues"
  },
  "homepage": "https://github.com/mhmdtshref/nodejs-starter#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^7.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "husky": "^7.0.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.7.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6"
  }
}
